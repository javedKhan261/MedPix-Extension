{
  "manifest_version": 2,

  "name": "Curofy Medpix Extension",
  "description": "This extension fetches medpix case",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab","tabs","storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://medpix.nlm.nih.gov/*","https://app.figure1.com/*","https://admin.curofy.com/*","http://127.0.0.1:8000/*"],
      "js": ["jquery-3.2.1.min.js","myScript.js"]
    }
  ]
}
