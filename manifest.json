{
  "manifest_version": 2,

  "name": "Curofy Discussion Extension",
  "description": "This extension fetches medpix case",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
        "16": "images/fig1-ext-1x.png",
        "48": "images/fig1-ext-2x.png",
        "128": "images/fig1-ext-3x.png"
    },
    "default_popup": "popup.html",
    "persistent": true
  },
  "icons": {
    "16": "images/fig1-ext-1x.png",
    "48": "images/fig1-ext-2x.png",
    "128": "images/fig1-ext-3x.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab","tabs","storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://medpix.nlm.nih.gov/*","https://app.figure1.com/*","https://admin.curofy.com/*","http://127.0.0.1:8000/*"],
      "js": ["jquery-3.2.1.min.js","myScript.js"]
    }
  ]
}
